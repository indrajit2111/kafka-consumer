<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <!-- Suppress these files because Spring Boot needed them -->
    <suppress checks="FinalClass" files="MainApp.java"/>
    <suppress checks="HideUtilityClassConstructor" files="MainApp.java"/>
    <suppress checks="FinalClass" files=".*ServiceImpl.java"/>
    <suppress checks="FinalClass" files=".*RepositoryImpl.java"/>
    <suppress checks="ParameterNumber" files=".*Dao.java"/>
    <suppress checks="ParameterNumber" files=".*Controller.java"/>
    <suppress checks="ParameterNumber" files=".*Request.java"/>
    <suppress checks="ParameterNumber" files=".*Response.java"/>
    <suppress checks="ParameterNumber" files="SwaggerConfig.java"/>
    <suppress checks="IllegalThrows" files="MethodDetailsAdvisor.java"/>
    <suppress checks="IllegalToken" files="Util.java"/>
    <suppress checks="IllegalCatch" files="MethodDetailsAdvisor.java"/>
    <suppress checks="ParameterNumber" files=".*model*."/>
    <suppress checks="ParameterNumber" files=".*dto*."/>
    <suppress checks="[a-zA-Z0-9]*" files=".*model*."/>
    <suppress checks="[a-zA-Z0-9]*" files=".*dto*."/>
    <suppress checks="RegexpSingleline" files="JsonReader.java"/>
</suppressions>
